.data
.text
	li $v0,5
	syscall
	move $t0, $v0		#input
	li $t1, 1
	li $t2, 2
	li $t3, 3
	li $t4, 4
	li $t5, 200		#b
	li $t6, 4		#c
	beq $t0, $t1, case1
	beq $t0, $t2, case2
	beq $t0, $t3, case3
	beq $t0, $t4, case4
default:li $a0,0
	j exit
case1: 	add $a0,$t5,$t6
	j exit
case2: 	sub $a0, $t5, $t6
	j exit
case3: 	mul $a0, $t5, $t6
	j exit
case4: div $a0, $t5, $t6
	j exit
exit: 	li $v0,1
	syscall